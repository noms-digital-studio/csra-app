{
    "extends": "airbnb",
    "env": {
      "node": true,
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "import/no-extraneous-dependencies": ["error", {"devDependencies": true}],
      "import/extensions": ["error", "always", { "js": "never", "jsx": "never" }]
    },
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ]
}
