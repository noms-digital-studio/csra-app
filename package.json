{
  "name": "csra-application",
  "version": "0.0.1",
  "description": "Cell Sharing risk assessment",
  "main": "index.js",
  "directories": {
    "test": "test",
    "src": "src",
    "server": "sever"
  },
  "nodemonConfig": {
    "ignore": [
      ".circleci/*",
      "migrations/*",
      "node_modules/*",
      "test/*"
    ],
    "delay": "2500"
  },
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "rimraf public/dist && ./bin/build-css && DEBUG=csra* nodemon ./bin/www | bunyan -o short",
    "lint": "eslint . --cache --max-warnings 0",
    "test": "mocha test/*.spec.js test/**/*.spec.js test/unit/**/*.spec.js test/unit/**/**/*.spec.js test/client/**/*.spec.js test/client/**/**/*.spec.js",
    "verify": "snyk test && yarn lint && yarn test",
    "snyk-test": "snyk test",
    "test:watch": "yarn test -- --watch",
    "test:integration": "yarn test:e2e && yarn test:rest",
    "test:rest": "mocha test/end-to-end/rest/*.spec.js",
    "test:db": "mocha test/end-to-end/db/*.spec.js",
    "test:e2e": "wdio wdio.conf.js",
    "test:smoke": "SMOKE_TEST_PATH='./test/end-to-end/smoke/*.spec.js' wdio wdio.conf.js",
    "build": "./bin/build-css && webpack",
    "record-build-info": "node ./bin/record-build-info",
    "wait-for-deploy": "node ./bin/wait-for-deploy",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">= 8.4.0",
    "npm": ">=5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "foo.com"
  },
