#!/usr/bin/env node

require('dotenv').config();
require('babel-register')();

const { seedDbWithViperData } = require('../db/seedUtils');

const seedsToAdd = Number(process.env.SEED_AMOUNT) || 100;

console.log('Seeding the database... Please wait...');

seedDbWithViperData(seedsToAdd).then(() => {
  console.log('Seeding complete.');
  console.log('Now exiting');
  process.exit(0);
});
